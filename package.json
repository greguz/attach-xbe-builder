{
  "private": true,
  "name": "attach-xbe-builder",
  "version": "0.1.0",
  "description": "Creates a customized attach.xbe from a game's default.xbe",
  "type": "module",
  "scripts": {
    "build": "rollup -c && pkg ./build/build-attach-xbe.cjs -t node18-linux-x64,node18-macos-x64,node18-win-x64 --out-path ./build"
  },
  "keywords": [
    "xbox",
    "xbe",
    "attach",
    "driveimageutils"
  ],
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "devDependencies": {
    "pkg": "^5.8.0",
    "rollup": "^3.17.3",
    "standard": "^17.0.0"
  }
}
